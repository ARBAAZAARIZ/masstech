package mypack;


class Parent{
	String name;
	int id;
	
	public Parent(String name,int id) {
		this.name=name;
		this.id=id;
		
	}
	
	public void display() {
		System.out.println(name + " " +id);
	}
}

public class Impl extends Parent {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		Impl obj=new Impl(20000,"Jhon",22);
	}
	double salary;
	
	public Impl(double salary,String name,int id) {
		super(name, id);
		this.salary=salary;
	}
	
	public void display() {
		System.out.println(salary);
		super.display();
	}

}
